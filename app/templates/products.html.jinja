{% extends "base.html.jinja" %}
{% block content %}
    <h2>List of products</h2>
    <ul class="list-group">
    {% for product in products %}
        <li class="list-group-item"><a href="{{url_for('product', product_id = product)}}">{{product}}</a></li>
    {% endfor %}
    </ul>
{% endblock content %}